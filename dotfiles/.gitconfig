[user]
    name = Satcomx00
    email = noreply@gmail.com

[rerere]
    enabled = true

[core]
    editor = code --wait

[alias]
    s = status
    co = checkout
    br = branch
    ci = commit
    st = status
    unstage = reset HEAD --
    last = log -1 HEAD
    lg = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit

[pull]
    rebase = false

[init]
    defaultBranch = main
